<%
IMPORT FILE "common_gch.gch";
var FP_ERRORSTR     = "SUCC";
var FP_OBJNAME      = "OBJ_DEVPING_ID";
var FP_PARANUM      = 2;
var FP_MULTIDISPLAY = 0;
var FP_TIME         = 8;
var FP_IDLE         = "";
var FP_HANDLE;
var PARA[2] =
{
"Host",
"Interface"
};
var timer=request("IF_TIME");
if(timer!=""&&timer!=NULL)
{
FP_TIME  = request("IF_TIME");
}
var idle = request("IF_IDLE");
if(idle=="submit")
{
FP_IDLE=idle;
}
var FP_ACTION = request("IF_ACTION");
create_form_start("fSubmit", "");
createSingleInstHidden(PARA, FP_PARANUM);
if("apply" == FP_ACTION || "new" == FP_ACTION)
{
FP_HANDLE = create_paralist();
for(var i = 0; i < FP_PARANUM; i++)
{
setpara(FP_HANDLE, PARA[i]);
}
set_para(FP_HANDLE, "NumofRepeat", "4");
set_para(FP_HANDLE, "DataBlockSize", "56");
set_para(FP_HANDLE, "DiagnosticsState", "Requested");
set_para(FP_HANDLE, "Timeout", "2000");
set_inst(FP_HANDLE, FP_OBJNAME, "");
FP_ERRORSTR = getNewInstError(FP_ERRORSTR);
}
undoDBSave();
if (FP_MULTIDISPLAY == 0)
{
FP_HANDLE = create_paralist();
get_inst(FP_HANDLE, FP_OBJNAME, "");
cover_hidden(FP_HANDLE, PARA, FP_PARANUM);
destroy_paralist(FP_HANDLE);
}
getDisplayInstError(FP_ERRORSTR);
create_hidden_sep("IF_TIME", FP_TIME);
create_hidden_sep("IF_IDLE", FP_IDLE);
create_form_end();
%>
