<%
var buttoncss   = "button3";
if(getenv("LanguageCtr") == "rus")
{
buttoncss   = "button6";
}
%>
<tr>
<td class="td3" colspan="2">
<input id="Btn_Refresh" name="Btn_Refresh" type="submit" class="<%=buttoncss;%>" value="&?btn001;" onClick="LogRefreshSubmit();" />&nbsp;&nbsp;&nbsp;
<input id="Btn_Clear" name="Btn_Clear" type="submit" class="<%=buttoncss;%>" value=" &?log_m011; " onClick="LogClearSubmit();" />
</td>
</tr>
<%
IMPORT FILE "auth/api.gch";
var session_token = get_csrf_session_token();
