<!--bridge-->
<%
var BRIDGE_NUM,BRIDGE_OBJNAME,BRIDGE_IDENTITY,BRIDGE_HANDLE;
var BRIDGE_WANCName;
BRIDGE_OBJNAME="OBJ_ETHWANCBRIDGE_ID";
BRIDGE_NUM = query_list(BRIDGE_OBJNAME, "IGD");
for(var i=0;i<BRIDGE_NUM;i++)
{
