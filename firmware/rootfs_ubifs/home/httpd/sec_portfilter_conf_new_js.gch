{
cellId = "Line_" + index + "_" + FWIPSTATIC_PARA[i] + "_" + i;
if(7 == i)
{
cellSize = 12;
}
else
if(11 == i)
{
cellSize = 8;
}
else
{
cellSize = 6;
}
if(1 == i || 3 == i)
{
cellValue = TransferList(i, getValue(FWIPSTATIC_PARA[i] + index));
}
else
{
cellValue = getValue(FWIPSTATIC_PARA[i] + index);
}
