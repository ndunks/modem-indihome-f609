function changeProtocol()
{
if(getValue("Frm_Protocol") == 3 || getValue("Frm_Protocol") == 4)
{
setValue("Frm_MinSrcPort", "");
setValue("Frm_MaxSrcPort", "");
setValue("Frm_MinDstPort", "");
setValue("Frm_MaxDstPort", "");
