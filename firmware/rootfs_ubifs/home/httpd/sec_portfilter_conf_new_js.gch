ShowErrorForCom("Frm_MinDstIp", null, msg);
return false;
}
}
else
if( getValue("Frm_MinDstIp") == "" && getValue("Frm_MaxDstIp") != "" )
{
msg = getmsg(158);
ShowErrorForCom("Frm_MinDstIp", null, msg);
return false;
}
