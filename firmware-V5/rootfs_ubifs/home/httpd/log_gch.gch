<%
IMPORT FILE "common_gch.gch";
var FP_ERRORSTR = "SUCC";
var FP_OBJNAME  = "OBJ_LOG_ID";
var FP_PARANUM  = 4;
var FP_IDENTITY = "";
var FP_INDEX    = getInstIndex("new");
var FP_MULTIDISPLAY = 0;
var FP_HANDLE;
var PARA[4] =
{
"LogEnable",
"LogLevel",
"ServiceIp",
"ServiceEnable"
};
var FP_ACTION = request("IF_ACTION");
create_form_start("fSubmit", "");
createSubmitHidden(FP_MULTIDISPLAY, PARA, FP_PARANUM, FP_INDEX);
create_hidden_sep("IF_INDEX", FP_INDEX);

FP_ERRORSTR = applyOrNewOrDelInst(FP_OBJNAME, FP_ACTION, FP_IDENTITY, PARA, FP_PARANUM, FP_ERRORSTR);
if(FP_ACTION == "clearlog")
{
FP_HANDLE = create_paralist();
set_para(FP_HANDLE, "CmdId", "cmd_clearlog");
dev_action(FP_HANDLE);
}
FP_ERRORSTR = getDisplayInstError(FP_ERRORSTR);
displayInst(FP_OBJNAME, FP_MULTIDISPLAY, PARA, FP_PARANUM, FP_ERRORSTR);

create_form_end();
%>
