<%
IMPORT FILE "common_gch.gch";
function ad_func(FP_ACTION, CONFIG_IDENTITY, FP_ERRORSTR)
{
var FP_OBJNAME      = "OBJ_WLANAD_ID";
var FP_MULTIDISPLAY = 1;
var FP_PARANUM      = 13;
var PARA[13] =
{
"DeviceName",
"ADMACAddress",
"ADIPAddress",
"ADAuthState",
"LastRequestedUnicastCipher",
"LastRequestedMulticastCipher",
"LastPMKId",
"LastDataTransmitRate",
"MCS",
"RSSI",
"TXRate",
"RXRate",
"CurrentMode"
};
create_hidden_newpara(PARA, FP_PARANUM);
var FP_INSTNUM = query_list(FP_OBJNAME, CONFIG_IDENTITY);
if (FP_MULTIDISPLAY == 1)
{
for (var i = 0; i < FP_INSTNUM; i++)
{
var FP_HANDLE = create_paralist();
var FP_IDENTITY = query_identity(i);
get_inst(FP_HANDLE, FP_OBJNAME, FP_IDENTITY);
create_hidden_multiinstpara(FP_HANDLE, PARA, i, FP_PARANUM);
destroy_paralist(FP_HANDLE);
}
}
set_hidden("IF_INSTNUM", FP_INSTNUM);
return getDisplayInstError(FP_ERRORSTR);
}
%>
