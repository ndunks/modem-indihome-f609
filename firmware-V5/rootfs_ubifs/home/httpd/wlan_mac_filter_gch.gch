<%
IMPORT FILE "common_gch.gch";
function wlan_mac(FP_ACTION, FP_WLANMAC_TAG, FP_INDEX,FP_ERRORSTR)
{
var FP_OBJNAME      = "OBJ_ACLCFG_ID";
var FP_MULTIDISPLAY = 1;
var FP_PARANUM      = 3;
var PARA[3] =
{
"ViewName",
"Interface",
"MACAddress"
};
if (FP_INDEX IS NULL)
{
FP_INDEX = -1;
}
if (FP_WLANMAC_TAG IS NULL)
{
FP_WLANMAC_TAG = "N";
}
if("basic_apply" == FP_ACTION)
{
FP_ACTION = "apply";
}
else if("apply" == FP_ACTION)
{
FP_ACTION = "";
}
create_hidden_newpara(PARA, FP_PARANUM);
var FP_IDENTITY = getInstIdentityByAction(FP_OBJNAME, FP_MULTIDISPLAY, FP_ACTION, FP_INDEX);

FP_ERRORSTR = getDisplayInstError(FP_ERRORSTR);
if("Y" == FP_WLANMAC_TAG)
{
FP_ERRORSTR = applyOrNewOrDelInst(FP_OBJNAME, FP_ACTION, FP_IDENTITY, PARA, FP_PARANUM, FP_ERRORSTR);
}
FP_ERRORSTR = displayFunctionInst(FP_OBJNAME, FP_MULTIDISPLAY, PARA, FP_PARANUM, FP_ERRORSTR);
return FP_ERRORSTR;
}
%>
