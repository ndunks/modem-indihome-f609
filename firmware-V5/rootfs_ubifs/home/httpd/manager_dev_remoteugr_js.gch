function myDoChange(flag)
{
}
function myRemoteCfgUpgrade()
{
var msg = "";
var cfgUrl = trimAllSpaces(getValue("Frm_UserCfgUrl"));
if (checkURL(cfgUrl) != true)
{
msg = getmsg(105);
ShowErrorForCom("Frm_UserCfgUrl","Fnt_UserCfgUrl",msg);
return false;
}
if (checkStrLengthRange(getValue("Frm_UserCfgUrl"),1,256) != true)
{
msg = getmsg(102,1,256);
ShowErrorForCom("Frm_UserCfgUrl","Fnt_UserCfgUrl",msg);
return false;
}
jslSetValue("RemoteUserCfgUrl","Frm_UserCfgUrl");
setValue("UpgradeFileType","4");
setValue("IF_ACTION", "apply");
jslDisable("Frm_UserCfgUrl","Btn_UserCfg","Frm_FirmwareUrl","Btn_Firmware");
DisableALL();
getObj("FileUploading").style.display = "";
getObj("fSubmit").submit();
}
function myRemoteFmwUpgrade()
{
var msg = "";
var firmWareUrl = trimAllSpaces(getValue("Frm_FirmwareUrl"));
if (checkURL(firmWareUrl) != true)
{
msg = getmsg(105);
ShowErrorForCom("Frm_FirmwareUrl","Fnt_FirmwareUrl",msg);
return false;
}
if (checkStrLengthRange(getValue("Frm_FirmwareUrl"),1,256) != true)
{
msg = getmsg(102,1,256);
ShowErrorForCom("Frm_FirmwareUrl","Fnt_FirmwareUrl",msg);
return false;
}
jslSetValue("RemoteFirmwareUrl","Frm_FirmwareUrl");
setValue("UpgradeFileType","1");
setValue("IF_ACTION", "apply");
jslDisable("Frm_UserCfgUrl","Btn_UserCfg","Frm_FirmwareUrl","Btn_Firmware");
DisableALL();
getObj("FileUploading").style.display = "";
getObj("fSubmit").submit();
}
function pageGetValue()
{
setValue("Frm_FirmwareUrl",getValue("RemoteFirmwareUrl"));
setValue("Frm_UserCfgUrl",getValue("RemoteUserCfgUrl"));
}
function pageLoad(url)
{
<%
